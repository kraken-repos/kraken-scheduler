	String finalizedValue = vars.get(&quot;finalizedValue&quot;);
	JsonSlurper jsonSlurper = new JsonSlurper();
	def response = response = jsonSlurper.parseText(finalizedValue);
	def failureMessage = &quot;&quot;;
	try{
	if(response.id &lt;= 0){
		failureMessage += &quot;Your Finalized Response id is Invalid \n&quot;;
	}
	}catch(Exception e){
		failureMessage += &quot;Your Finalized Response id is Invalid \n &quot;;
	}
	String status = response.status;
	if(status == null || !status.equals(&quot;FINALIZED&quot;))
		failureMessage += &quot;Your Finalized Response status is Invalid \n&quot;
	if(failureMessage?.trim() &amp;&amp; failureMessage.trim().length() != 0 &amp;&amp; enabled){
		AssertionResult.setFailureMessage(failureMessage);
		AssertionResult.setFailure(true);
	}
}
