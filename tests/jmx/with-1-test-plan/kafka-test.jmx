import org.apache.jmeter.services.FileServer
import org.apache.jmeter.gui.GuiPackage;
import org.apache.commons.io.FilenameUtils;

String testPlanFileDir = FileServer.getFileServer().getBaseDir();
vars.put(&quot;testPlanFileDir&quot;, testPlanFileDir);

File file = new File(testPlanFileDir + &quot;/jmeter.properties&quot;);
InputStream configInputStream = new FileInputStream(file);
Properties configProperties = new Properties();
configProperties.load(configInputStream);

String enabled = configProperties.getOrDefault(&quot;jmeter.enabled&quot;,&quot;false&quot;);
vars.put(&quot;jmeter.enabled&quot;,enabled);
boolean jmeterEnabled = Boolean.valueOf(enabled);
vars.putObject(&quot;testingConfigProperties&quot;,configProperties);

if(jmeterEnabled){

Properties kakfkaConncetionProps = new Properties();
	if(configProperties.get(&quot;kafka.bootstrap.servers&quot;) == null){
		String kafakconfigLocation = configProperties.getOrDefault(&quot;kafka.config.location&quot;,&quot;/etc/secrets/kafka/kafka.properties&quot;);
		File kafkaConfigFile = new File(kafakconfigLocation);
		InputStream kafkaConfigInputStream = new FileInputStream(kafkaConfigFile);
		Properties kafkaBasicProperties = new Properties();
		kafkaBasicProperties.load(kafkaConfigInputStream);
			
		String mechanism = &quot;PLAIN&quot;;
		String protocol = kafkaBasicProperties.getOrDefault(&quot;KAFKA_PROTOCOL&quot;, &quot;SASL_SSL&quot;); //&quot;PLAINTEXT&quot;
		String user = kafkaBasicProperties.get(&quot;KAFKA_USER&quot;);
		String password =  kafkaBasicProperties.get(&quot;KAFKA_PASSWD&quot;);
 		String salConfig = &quot;org.apache.kafka.common.security.plain.PlainLoginModule required username=\&quot;&quot; + user + &quot;\&quot; password=\&quot;&quot; + password + &quot;\&quot;&quot; + &quot;;&quot; ;
 		String brokers = kafkaBasicProperties.get(&quot;KAFKA_HOST&quot;) + &quot;:&quot; + kafkaBasicProperties.get(&quot;KAFKA_PORT&quot;);
 		kakfkaConncetionProps.put(&quot;bootstrap.servers&quot;,brokers);
 		kakfkaConncetionProps.put(&quot;security.protocol&quot;,protocol);
 		kakfkaConncetionProps.put(&quot;sasl.mechanism&quot;,mechanism);
 		kakfkaConncetionProps.put(&quot;sasl.jaas.config&quot;,salConfig);
	}
	else{
		kakfkaConncetionProps.put(&quot;bootstrap.servers&quot;,configProperties.get(&quot;kafka.bootstrap.servers&quot;));
		kakfkaConncetionProps.put(&quot;security.protocol&quot;,configProperties.get(&quot;kafka.security.protocol&quot;));
		kakfkaConncetionProps.put(&quot;sasl.mechanism&quot;,configProperties.get(&quot;kafka.sasl.mechanism&quot;));
		kakfkaConncetionProps.put(&quot;sasl.jaas.config&quot;,configProperties.get(&quot;kafka.sasl.jaas.config&quot;));
	}
 	
 	kakfkaConncetionProps.put(&quot;group.id&quot;,configProperties.getOrDefault(&quot;kafa.groupId&quot;,&quot;jmeter-managment&quot;));
 	kakfkaConncetionProps.put(&quot;enable.auto.commit&quot;,configProperties.getOrDefault(&quot;kafka.enable.auto.commit&quot;,&quot;true&quot;));
	kakfkaConncetionProps.put(&quot;acks&quot;, configProperties.getOrDefault(&quot;kafka.acks&quot;,&quot;all&quot;));
	kakfkaConncetionProps.put(&quot;retries&quot;, configProperties.getOrDefault(&quot;kafka.retries&quot;,0));
	kakfkaConncetionProps.put(&quot;auto.commit.interval.ms&quot;,configProperties.getOrDefault(&quot;kafka.auto.commit.interval.ms&quot;,&quot;1000&quot;));
 	kakfkaConncetionProps.put(&quot;session.timeout.ms&quot;, configProperties.getOrDefault(&quot;kafka.session.timeout.ms&quot;,&quot;120000&quot;));
 	kakfkaConncetionProps.put(&quot;key.serializer&quot;,
 		configProperties.getOrDefault(&quot;kafka.key.serializer&quot;,&quot;org.apache.kafka.common.serialization.StringSerializer&quot;));
 	kakfkaConncetionProps.put(&quot;value.serializer&quot;,
 		configProperties.getOrDefault(&quot;kafka.value.serializer&quot;,&quot;org.apache.kafka.common.serialization.StringSerializer&quot;));
 	
 	kakfkaConncetionProps.put(&quot;batch.size&quot;,configProperties.getOrDefault(&quot;kafka.batch.size&quot;,&quot;16384&quot;));
 	kakfkaConncetionProps.put(&quot;send.buffer.bytes&quot;,configProperties.getOrDefault(&quot;kafka.send.buffer.bytes&quot;,&quot;131072&quot;));
 	kakfkaConncetionProps.put(&quot;receive.buffer.bytes&quot;,configProperties.getOrDefault(&quot;kafka.receive.buffer.bytes&quot;, &quot;32768&quot;));
 	kakfkaConncetionProps.put(&quot;buffer.memory&quot;,configProperties.getOrDefault(&quot;kafka.buffer.memory&quot;,&quot;33554432&quot;))
 
 	kakfkaConncetionProps.put(&quot;key.deserializer&quot;,
		configProperties.getOrDefault(&quot;kafka.key.deserializer&quot;,&quot;org.apache.kafka.common.serialization.StringDeserializer&quot;));
 	kakfkaConncetionProps.put(&quot;value.deserializer&quot;,
 		configProperties.getOrDefault(&quot;kafka.value.deserializer&quot;,&quot;org.apache.kafka.common.serialization.StringDeserializer&quot;));

	vars.putObject(&quot;kafkaConncetionProps&quot;,kakfkaConncetionProps);
	log.info(kakfkaConncetionProps.get(&quot;bootstrap.servers&quot;));
 	log.info(&quot;================= Properties loadding completes =================&quot;)
}


