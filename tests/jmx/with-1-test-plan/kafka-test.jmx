          <stringProp name="script">import org.apache.kafka.clients.consumer.*
import org.apache.kafka.clients.consumer.KafkaConsumer
import org.apache.kafka.clients.producer.KafkaProducer
import org.apache.kafka.clients.producer.ProducerConfig
import org.apache.kafka.clients.producer.ProducerRecord
import org.apache.kafka.common.serialization.LongDeserializer
import org.apache.kafka.common.serialization.LongSerializer
import org.apache.kafka.common.serialization.StringDeserializer
import org.apache.kafka.common.serialization.StringSerializer
import org.apache.kafka.common.header.*;
import org.apache.kafka.common.TopicPartition;
import groovy.json.JsonSlurper;

if(Boolean.valueOf(vars.get(&quot;jmeter.enabled&quot;))){
	Properties props = (Properties)vars.getObject(&quot;kafkaConncetionProps&quot;);

	if(props != null){
	KafkaConsumer&lt;String, String&gt; consumer = new KafkaConsumer&lt;String, String&gt;(props);
	TopicPartition partition0 = new TopicPartition(&quot;ClaimRequestFinalized&quot;, 0);
     consumer.subscribe(Arrays.asList(&quot;ClaimRequestFinalized&quot;));
	log.info(&quot;Subscribed to topic &quot; + &quot;ClaimRequestFinalized&quot;);
	int i = 0;
	long time =  System.currentTimeMillis();
	long end = time + 20000;
	final int resultSize = 1;
	boolean isCompleted = false;
	JsonSlurper jsonSlurper = new JsonSlurper();
	boolean flag = true;
	while(time &lt; end &amp;&amp; !isCompleted){
	ConsumerRecords&lt;String, String&gt; records = consumer.poll(100);
	if(flag){
		consumer.seek(partition0,0)
		flag = false;	
	}
	if(records.count() != 0 ){
		 for (ConsumerRecord&lt;String, String&gt; record : records){
			log.info(&quot;offset = {}, key = {}, value = {}\n&quot;, record.offset(), record.key(), record.value());
			def values = jsonSlurper.parseText(record.value());
			int id = Integer.valueOf(vars.get(&quot;claimRequestId&quot;));
			if(values.id == id){
				vars.put(&quot;finalizedValue&quot;,record.value());
			isCompleted = true;
			break;
			}
		}	
	 }    
		time = System.currentTimeMillis();
	}
	
	consumer.close();
	}
}

